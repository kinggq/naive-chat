{
  "name": "naive-chat",
  "version": "0.1.12",
  "private": false,
  "packageManager": "pnpm@8.6.0",
  "main": "dist/naive-chat.mjs",
  "types": "dist/packages/index.d.ts",
  "scripts": {
    "build:lib": "vite build",
    "build": "vite build --config vite.config.docs.ts",
    "build:dts": "tsc --emitDeclarationOnly",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@iconify-json/bi": "^1.1.17",
    "@iconify-json/carbon": "^1.1.17",
    "@iconify-json/mdi": "^1.1.53",
    "@iconify-json/ri": "^1.1.10",
    "@types/node": "^20.2.5",
    "@unocss/eslint-config": "^0.53.0",
    "@unocss/reset": "^0.53.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue-macros/volar": "^0.10.2",
    "@vue/test-utils": "^2.3.2",
    "eslint": "^8.42.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.2",
    "pnpm": "^8.6.0",
    "taze": "^0.10.2",
    "typescript": "^5.1.3",
    "unocss": "^0.53.0",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.25.0",
    "unplugin-vue-macros": "^2.2.1",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^3.4.0",
    "vitest": "^0.31.4",
    "vue-tsc": "^1.6.5"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "eslintConfig": {
    "extends": [
      "@antfu",
      "@unocss"
    ]
  }
}
